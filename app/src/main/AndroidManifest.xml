<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.responsecounter">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".HomeActivites.PhysiciansSubjectHome"></activity>
        <activity android:name=".MiscellaneousActivites.settingsActivity" />
        <activity android:name=".SignInActivities.SignInActivity" />
        <activity android:name=".SignUpActivities.SubjectSignUpActivity" />
        <activity android:name=".HomeActivites.PhysicianHome" />
        <activity android:name=".MiscellaneousActivites.HelpActivity" />
        <activity android:name=".MiscellaneousActivites.NoteActivity" />
        <activity android:name=".TestActivities.SingleButtonActivity" />
        <activity android:name=".SignUpActivities.PhysicianSignUpActivity" />
        <activity android:name=".HomeActivites.SubjectHome" />
        <activity android:name=".TestActivities.DuelButtonActivity" />
        <activity android:name=".MiscellaneousActivites.ReportActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>